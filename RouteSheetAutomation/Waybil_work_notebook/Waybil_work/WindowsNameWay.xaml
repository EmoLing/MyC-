<Window x:Class="Waybil_work.WindowsNameWay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Waybil_work"
        mc:Ignorable="d"
        Title="WindowsNameWay" Height="150.5" Width="200">
    <Grid>
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="10,74,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="Button_Click"/>
        <Label Content="Номер маршрутного листа" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="162"/>
        <TextBox x:Name="BoxName" HorizontalAlignment="Left" Height="22" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162"/>
        <Button x:Name="ButCancel" Content="Отмена" HorizontalAlignment="Left" Margin="97,74,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="ButCancel_Click"/>

    </Grid>
</Window>
